#!/usr/bin/env bash
#this script counts to 20 with notes for bad lunch numbers
i="1"
while [ "$i" -le "20" ]
do
    echo "$i"
    case $i in
	4)
	    echo "bad luck from China"
	    ;;
	9)
	    echo "bad luck from Japan"
	    ;;
	17)
	    echo "bad luck from Italy"
	    ;;
    esac
    i=$((i+1))
done
	      
